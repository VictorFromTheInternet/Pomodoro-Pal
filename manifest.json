{
  "manifest_version": 3,
  "name": "Pomodoro Pal",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys", 
  "icons": {
    "16": "Pomodoro_Pal_16.png",
    "32": "Pomodoro_Pal_32.png",
    "48": "Pomodoro_Pal_48.png",
    "128": "Pomodoro_Pal_128.png"
  },
  "background":{
    "service_worker":"service-worker.js", 
    "type":"module"    
  },
  "action":{},
  "permissions":[
    "scripting"
  ],
  "host_permissions": [
    "http://*/*", 
    "https://*/*"
  ]
}